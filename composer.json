{
    "name": "andydune/hypercomments",
    "description": "Hypercomments API.",
    "license": "MIT",
    "keywords": [
        "php",
        "comments",
        "api"
    ],
    "authors": [
        {
            "name": "Andrey Ryzhov",
            "email": "info@rznw.ru"
        }
    ],
    "homepage": "https://github.com/AndyDune/Hypercomments",
    "autoload": {
        "psr-4": {
            "AndyDune\\Hypercomments\\": "src/"
        }
    },
    "require": {
        "php": "^7.0 || ^7.1 || ^7.2",
        "andydune/pipeline": "^1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.15 || ^6.0.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": "true"
    },
    "autoload-dev": {
        "psr-4": {
            "AndyDune\\HypercommentsTest\\": "test/"
        }
    }
}
